<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD configuration 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
    <setting name="cacheEnabled" value="true" /><!-- flushCache="true" -->
    <setting name="mapUnderscoreToCamelCase" value="true" />
    <setting name="jdbcTypeForNull" value="VARCHAR" />
    <setting name="logImpl" value="NO_LOGGING" />
    <setting name="defaultStatementTimeout" value="600" />
    <!-- <setting name="resultSetType" value="SCROLL_INSENSITIVE" /> --><!-- FORWARD_ONLY|SCROLL_SENSITIVE|SCROLL_INSENSITIVE -->
  </settings>
</configuration>